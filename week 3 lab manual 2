#include<stdio.h>
int main()
{
    int arr[]={202, 104, 304, 223, 453, 123};
    int search =304;
    int i;
    for(i=0;i<=5;i++)
    {
        if(arr[i]==search)
        {
            printf("position =%d row\n",i);
            return 0;
        }
    }
    printf("product not available\n");
    return 0;
}
